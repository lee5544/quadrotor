    </model>


    <!-- 人 -->
    <model name='person_walking1'>
      <include>
        <uri>model://person_walking</uri>
	      <pose> -2 -3 0.5 0 0 0.0</pose>
      </include>
    </model>
    <model name='person_walking2'>
      <include>
        <uri>model://person_walking</uri>
	      <pose> 2 4 0.5 0 0 -1.57</pose>
