    <!-- 柱子障碍物 -->
    <model name='obstacle'>
      <static>true</static>
      <link name='pillar_1'>
        <pose frame=''>-1 -1.0 1.0 0 -0 0</pose>
        <visual name='visual'>
          <geometry>
            <cylinder>
              <radius>0.2</radius>
