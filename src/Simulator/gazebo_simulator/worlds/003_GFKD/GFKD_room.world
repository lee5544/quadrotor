    </model>

    <!-- 会议桌 -->
    <model name='meeting_table'>
      <include>
        <uri>model://table_chair</uri>
	      <pose> 2 5 0.01 0 0 1.57</pose>
      </include>
    </model>

    <!-- 办公桌 -->
    <model name='table1'>
      <include>
        <uri>model://table_chair</uri>
	      <pose> 4 -0 0.01 0 0 1.57</pose>
